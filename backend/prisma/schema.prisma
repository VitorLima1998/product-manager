generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model Product {
  id         Int      @id @default(autoincrement())
  name       String
  price      Float
  categoryId Int
  category   Category @relation(fields: [categoryId], references: [id])
}

model Category {
  id         Int      @id @default(autoincrement())
  name       String
  products   Product[]
}